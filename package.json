{
  "name": "express-production-server-template",
  "version": "1.0.0",
  "description": "Production Ready Express TypeScript Starter Template",
  "author": {
    "email": "rastogiharsh04@gmail.com",
    "name": "Harsh Rastogi"
  },
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "private": false,
  "scripts": {
    "start": "NODE_ENV=local nodemon",
    "start:debug": "NODE_ENV=local nodemon --inspect",
    "build:dev": "rm -rf build && webpack --config ./webpack/webpack.development.ts",
    "build:prod": "rm -rf build && webpack --config ./webpack/webpack.production.ts",
    "format": "prettier . --write --log-level=error",
    "lint": "npx eslint . --fix",
    "typecheck": "tsc --noEmit --pretty",
    "prepare": "husky install",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.8.10",
    "@types/webpack": "^5.28.4",
    "@types/webpack-dev-server": "^4.7.2",
    "@types/webpack-node-externals": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@webpack-cli/generators": "^3.0.7",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.53.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.0.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-middleware": "^6.1.1",
    "webpack-dev-server": "^4.15.1",
    "webpack-node-externals": "^3.0.0"
  }
}
